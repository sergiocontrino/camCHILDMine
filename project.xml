<project type="bio">
<property name="target.model" value="genomic"/>
<property name="common.os.prefix" value="common"/>
<sources>
  <source name="Demographics" type="delimited">
    <property name="delimited.dataSourceName" value="ChildrenData"/>
    <property name="delimited.dataSetTitle" value="Demographics"/>
    <property name="delimited.hasHeader" value="false"/>
    <property name="delimited.separator" value="tab"/>
    <property name="src.data.dir" location="data"/>
    <property name="delimited.includes" value="demographics.tsv"/>
    <property name="delimited.columns" value ="Patient.patientId,
    Patient.site,
    Patient.dateOfBirth,
    Patient.ageIn22,
    Patient.gender,
    Patient.indexMultipleDeprivation,
    Patient.ethnicity" />
  </source>
  <source name="HealthReferral" type="delimited">
    <property name="delimited.dataSourceName" value="ChildrenData"/>
    <property name="delimited.dataSetTitle" value="HealthReferral"/>
    <property name="delimited.hasHeader" value="false"/>
    <property name="delimited.separator" value="tab"/>
    <property name="src.data.dir" location="data"/>
    <property name="delimited.includes" value="health_referral.tsv"/>
    <property name="delimited.columns" value ="Patient.patientId,Referral.referralDate,Referral.teamReferredTo" />
  </source>
  <source name="HealthDiagnosis" type="delimited">
    <property name="delimited.dataSourceName" value="ChildrenData"/>
    <property name="delimited.dataSetTitle" value="HealthDiagnosis"/>
    <property name="delimited.hasHeader" value="false"/>
    <property name="delimited.separator" value="tab"/>
    <property name="src.data.dir" location="data"/>
    <property name="delimited.includes" value="health_diagnosis.tsv"/>
    <property name="delimited.columns" value ="Patient.patientId,
    Diagnosis.code,
    Diagnosis.name" />
  </source>
  <source name="SocialServices" type="delimited">
    <property name="delimited.dataSourceName" value="ChildrenData"/>
    <property name="delimited.dataSetTitle" value="SocialServices"/>
    <property name="delimited.hasHeader" value="false"/>
    <property name="delimited.separator" value="tab"/>
    <property name="src.data.dir" location="data"/>
    <property name="delimited.includes" value="socialservices_flags.tsv"/>
    <property name="delimited.columns" value ="Patient.patientId,
    Patient.safeguardingAlert,
    Patient.housingAlert" />
  </source>
</sources>
</project>
